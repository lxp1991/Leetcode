public class Solution {
    public int uniquePaths(int m, int n) {
    	if (m > 100 || n > 100) return 0;
    	int dp[][] = new int[m][n];
    	dp[0][0] = 1;
    	for (int i = 0; i < m; i++)
    		dp[i][0] = 1;
    	for (int i = 0; i < n; i++)
    		dp[0][i] = 1;
    	for (int i = 0; i < m; i++)
    		for (int j = 0; j < n; j++)
    			dp[i][j] = dp[i -1][j] + dp[i][j-1];
    	return dp[m-1][n-1];
    }
    
    
    public static void main(String[] args) {
		int m = 3, n = 3;
		System.out.println(new Solution().uniquePaths(m, n));
	}
}