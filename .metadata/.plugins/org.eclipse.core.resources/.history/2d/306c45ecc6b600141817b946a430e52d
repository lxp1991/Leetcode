
public class Solution {
	public static void merge(int A[], int m, int B[], int n) {
		int index = m + n + 1;
		int aIndex = m - 1;
		int bIndex = n - 1;
		while (aIndex >= 0 && bIndex >= 0) {
			if (B[bIndex] > A[aIndex]) A[index--] = B[bIndex--];
			else A[index--] = A[aIndex--];
		}
		
		while(aIndex >= 0) {
			A[index--] = A[aIndex--];
		}
		while(bIndex >= 0) {
			A[index--] = B[bIndex--];
		}
    }
	
	public static void main(String[] args) {
		int[] A = new int[10];
		for (int i = 0; i < 5; i++) {
			A[i] = 2 * i + 1;
		}
//		A[0] = 1;
//		A[1] = 6;
/*		A[2] = 1;
		A[3] = 1;
		A[4] = 1;*/
		
		int[] B = {1, 3, 6, 8,10};
		int m = 5, n = 5;
		merge(A, m, B, n);
		
		
	}
}
