public class Solution {
    public int uniquePathsWithObstacles(int[][] obstacleGrid) {
    	if (obstacleGrid == null) return 0;
    	int row = obstacleGrid.length;
    	int col = obstacleGrid[0].length;
    	int totalPath[][] = new int[row][col];
    	
    	for (int i = 0; i < row; i++) {
    		if (totalPath[i][0] == 1) 
    			totalPath[i][0] = 0;
    		else
    			totalPath[i][0] = 1;
    	}
    	for (int i = 0; i < col; i++) {
    		if (totalPath[0][i] == 1)
    			totalPath[0][i] = 0;
    		else
    			totalPath[0][i] = 1;
    	}
    	
    	for (int i = 1; i < row; i++)
    		for (int j = 1; j < col; j++) {
    			if (obstacleGrid[i - 1][j] == 1)
    				totalPath[i][j] = totalPath[i][j - 1];
    			if (obstacleGrid[i][j - 1] == 1)
    				totalPath[i][j] = totalPath[i - 1][j];
    			totalPath[i][j] = totalPath[i][j - 1] + totalPath[i - 1][j];
    		}
    	return totalPath[row - 1][col - 1];
    }

    public static void main(String[] args) {
    	int A[][] = {{0,0,0},
    				 {0,1,0},
    				 {0,0,0}};
    	System.out.println(new Solution().uniquePathsWithObstacles(A));
	}
}
