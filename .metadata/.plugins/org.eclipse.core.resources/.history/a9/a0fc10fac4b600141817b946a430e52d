
public class Solution {
	public static void merge(int A[], int m, int B[], int n) {
		int i = 0, j = 0, k,count;  
        while(j < n && i < m){  
            if(A[i] >= B[j]){  
                m++;  
                for(k = m-1; k > i; k--){  
                    A[k] = A[k-1];  
                }  
                A[i] = B[j];  
                i++;  
                j++;  
            }  
            else{  
                i++;  
            }  
              
        }  
        if(j < n){  
            while(j<n){  
             A[i] = B[j];  
             i++;  
             j++;  
            }  
        }  
    }
	
	public static void main(String[] args) {
		int[] A = new int[10];
		for (int i = 0; i < 5; i++) {
			A[i] = 2 * i + 1;
		}
//		A[0] = 1;
//		A[1] = 6;
/*		A[2] = 1;
		A[3] = 1;
		A[4] = 1;*/
		
		int[] B = {1, 3, 6, 8,10};
		int m = 5, n = 5;
		merge(A, m, B, n);
		
		
	}
}
