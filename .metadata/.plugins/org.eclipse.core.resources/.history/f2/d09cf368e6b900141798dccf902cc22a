    public Boolean traverse(TreeNode node, int rem) {
		if (node.left == null && node.right == null) {
			res -= node.val;
			if (res == 0) return true;
		}
		node.val;
		traverse(node.right, rem - node.val);
		traverse(node.left, rem - node.val);
		
		
	}
    
